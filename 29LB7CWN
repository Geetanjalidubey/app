<script type="text/javascript">
    /*$(window).on('load', function() {
        $('#herrising_popup').modal('show');
    });*/
   function checkEmailID(){
      var email = $("#email-id").val();
      $.ajax({
         url: 'welcome/chechEmailIdExitsOrNot',
         data: {email:email},
         type: 'POST',
         dataType: 'json',
      }).done(function(response){
         if(response.message == 'User present in system'){
            window.location.assign("https://www.jobsforher.com/login");
         } else if(response.message == 'User not present in the system'){
            window.location.assign("https://www.jobsforher.com/signup-with-email");
         }
      });
   }
   function getJobSearch(){
      var keyword = $("#keyword").val();
      var location = $("#location").val();
      if(keyword != "" && location == ""){
         window.open(
           'jobs?keyword='+keyword+'&button_type=&min_exp=-1&max_exp=-1',
           '_blank'
         );
      } else if(keyword == "" && location != ""){
         window.open(
           'jobs?keyword=&location%5B%5D='+location+'&button_type=&min_exp=-1&max_exp=-1',
           '_blank'
         );
      } else if(keyword != "" && location != ""){
         window.open(
           'jobs?keyword='+keyword+'&location%5B%5D='+location+'&button_type=&min_exp=-1&max_exp=-1',
           '_blank'
         );
      }
   }
</script>
